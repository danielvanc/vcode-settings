{
    "workbench.list.openMode": "doubleClick",
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "git.promptToSaveFilesBeforeCommit": true,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.copyOnSelection": true,
    "npm.enableScriptExplorer": true,
    "emmet.includeLanguages": {
        "twig": "html",
        "less": "less",
        "SCSS": "scss",
        "vue-html": "html",
        "javascript": "javascriptreact"
    },
    "git.autofetch": true,
    "editor.fontSize": 15,
    "editor.formatOnSave": true,
    "editor.formatOnSave": false,
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascript React]": {
        "editor.formatOnSave": false
    },
    "window.title": "${rootName}${separator}${activeEditorLong}",
    "window.nativeTabs": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "php.validate.executablePath": "/Applications/MAMP/bin/php/php7.0.13/bin/php",
    "php.executablePath": "/Applications/MAMP/bin/php/php7.0.13/bin/php",
    "emmet.showSuggestionsAsSnippets": true,
    "editor.snippetSuggestions": "top",
    "window.zoomLevel": 0,
    "terminal.integrated.fontSize": 15,
    "editor.tabSize": 2,
    "explorer.openEditors.visible": 0,
    "terminal.integrated.shell.osx": "/bin/zsh",
    // "terminal.integrated.fontFamily": "'Courier New', FiraFlott,Inconsolata-g for Powerline",
    // "terminal.integrated.fontFamily": "'Anonymous Pro',FiraFlott,Inconsolata-g for Powerline",
    "terminal.integrated.fontFamily": "FiraFlott,Inconsolata-g for Powerline",
    // "terminal.integrated.fontWeight": "600",
    "terminal.integrated.cursorBlinking": true,
    "workbench.fontAliasing": "antialiased",
    "editor.renderWhitespace": "none",
    "editor.multiCursorModifier": "alt",
    "editor.formatOnPaste": true,
    "editor.renderControlCharacters": false,
    "[twig]": {},
    "gitlens.keymap": "chorded",
    "editor.minimap.enabled": false,
    "editor.cursorStyle": "underline",
    "editor.formatOnType": false,
    "workbench.colorTheme": "Night Owl",
    "breadcrumbs.enabled": false,
    "editor.fontLigatures": true,
    "editor.fontFamily": "FiraFlott, Inconsolata-g for Powerline, Menlo, Monaco, 'Courier New', monospace",
    "editor.wordWrap": "on",
    "php.executablePath": "/usr/bin/php",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.activityBar.visible": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
}
