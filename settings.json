{
  "git.promptToSaveFilesBeforeCommit": "always",

  "gitlens.keymap": "chorded",

  "npm.enableScriptExplorer": true,

  "todo-tree.highlights.defaultHighlight": {
    "background": ""
  },

  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "twig": "html",
    "less": "less",
    "SCSS": "scss",
    "vue-html": "html",
    "javascript": "javascriptreact"
  },

  "files.associations": {
    "*.html": "twig",
    "*.js": "javascriptreact"
  },

  "git.autofetch": true,

  "[html-twig]": {
    "editor.tabSize": 2
  },

  "eslint.alwaysShowStatus": true,

  "carbon.theme": "dracula",

  "window.title": "${rootName}${separator}${activeEditorLong}",

  "php.validate.executablePath": "/usr/bin/php",

  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "'Dank Mono',FiraFlott,Inconsolata-g for Powerline",
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.copyOnSelection": true,

  "editor.multiCursorModifier": "alt",

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Dank Mono', 'JetBrains Mono', 'Fira Code', Hack,Hasklig, Inconsolata, monospace",
  "editor.fontSize": 19,
  "editor.fontLigatures": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",

  "workbench.colorTheme": "Moonlight Italic",
  "workbench.iconTheme": "helium-icon-theme",
  "workbench.list.openMode": "doubleClick",

  "workbench.colorCustomizations": {
    "editor.foldBackground": "#000"
  },
  "todohighlight.isEnable": true,
  "todo-tree.tree.showScanModeButton": false,
  "breadcrumbs.filePath": "off",

  "breadcrumbs.enabled": false,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,

  "window.zoomLevel": 0,
  "jest.showCoverageOnLoad": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "materialTheme.accent": "Remove accents",
  "terminal.integrated.lineHeight": 0,
  "workbench.fontAliasing": "antialiased",
  "editor.fontWeight": "500",
  "vscode_custom_css.imports": [
    // "file:///Users/dan/.vscode/extensions/webrender.synthwave-x-fluoromachine-0.0.12/synthwave-x-fluoromachine.css"
    // "file:///Users/dan/vscodestyles/synthwave84.css"
    "file:///Users/dan/vscodestyles/moonlight.css"
  ],
  "vscode_custom_css.policy": true
}
